{
  "name": "tabular-json",
  "version": "1.0.0",
  "description": "Tabular-JSON: a superset of JSON with tables and optional quotes",
  "keywords": [
    "Tabular-JSON",
    "JSON",
    "CSV",
    "NDJSON",
    "Table",
    "Extended",
    "Compact",
    "data format"
  ],
  "author": "Jos de Jong",
  "license": "ISC",
  "type": "module",
  "main": "lib/esm/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "types": "./lib/esm/index.d.ts"
    }
  },
  "scripts": {
    "dev": "astro dev",
    "preview": "astro preview",
    "build": "npm-run-all build:**",
    "_build:clean": "del-cli lib",
    "build:generate": "peggy --output ./src/scripts/lib/grammers/generated/tabular-json-parser.js --format es ./src/scripts/lib/grammers/tabular-json.pegjs --source-map",
    "build:astro:check": "astro check",
    "build:astro:build": "astro build",
    "_build:lib": "tsc",
    "test": "npm run build:generate && vitest",
    "test:ci": "npm run build:generate && vitest run",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "benchmark": "node benchmark/index.js"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/markdown-remark": "6.3.2",
    "@astrojs/svelte": "7.1.0",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@types/adm-zip": "0.5.7",
    "@types/git-diff": "2.0.7",
    "@types/node": "22.15.30",
    "adm-zip": "0.5.16",
    "astro": "5.9.0",
    "csv42": "5.0.3",
    "del-cli": "6.0.0",
    "fracturedjsonjs": "4.1.0",
    "git-diff": "2.0.6",
    "npm-run-all": "4.1.5",
    "peggy": "5.0.3",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-svelte": "3.4.0",
    "rehype-autolink-headings": "7.1.0",
    "sass": "1.89.1",
    "svelte": "5.33.14",
    "svelte-awesome": "3.3.5",
    "typescript": "5.8.3",
    "vitest": "3.2.2"
  }
}
